{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "domainSuffix": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "au",
        "nz",
        "com"
      ]
    },
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "shared.hero-section",
        "shared.quote",
        "shared.testimonial",
        "shared.testimonial-group",
        "common.navigator",
        "common.cta-callout",
        "common.featured-banner",
        "common.hero-with-testimonial",
        "common.featured-banner-split",
        "common.feature-banner-vertical-split",
        "common.testimonial-banner",
        "common.map",
        "common.footer",
        "common.text-list",
        "common.pricing-banner",
        "common.links",
        "common.heading",
        "common.free-block",
        "common.explainer-with-testimonials",
        "common.cta-callout-studio",
        "common.faq-banner",
        "common.faq-block",
        "common.faq-entry",
        "common.form-banner",
        "common.two-column-banner",
        "components.button",
        "common.feature-text-with-testimonials",
        "common.feature-image-with-testimonials",
        "common.free-block-with-images-banner"
      ],
      "required": true
    },
    "additionalCSS": {
      "type": "text"
    }
  }
}
