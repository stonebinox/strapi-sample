{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "domainSuffix": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "au",
        "nz"
      ]
    },
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "shared.hero-section",
        "shared.quote",
        "shared.testimonial",
        "shared.testimonial-group",
        "common.navigator",
        "common.cta-callout",
        "common.featured-banner",
        "common.hero-with-testimonial",
        "common.featured-banner-split",
        "common.feature-banner-vertical-split",
        "common.testimonial-banner",
        "common.map",
        "common.footer"
      ],
      "required": true
    },
    "pageStyle": {
      "type": "text"
    }
  }
}
